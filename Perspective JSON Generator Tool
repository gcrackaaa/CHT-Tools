			function createNode(element) {
			  return document.createElement(element);
			}

			function append(parent, el) {
			  return parent.appendChild(el);
			}

			function generatePerspectiveScript() {
			  var applicationInput = document.getElementById("applicationSelect").value;
			  	if (applicationSelect.selected == true){
			    applicationString = applicationJSON;
			    } else {
					    applicationString = "";
					};
			  var accountsubscriptionprojectInput = document.getElementById("accountsubscriptionprojectSelect").value;
			  	if (accountsubscriptionprojectSelect.selected == true){
			    accountsubscriptionprojectString = accountsubscriptionprojectJSON;
			    } else {
					    accountsubscriptionprojectString = "";
					};
			  var clientInput = document.getElementById("clientSelect").value;
			  	if (clientSelect.selected == true){
			    clientString = clientJSON;
			    } else {
					    clientString = "";
					};
			  var customerInput = document.getElementById("customerSelect").value;
			  	if (customerSelect.selected == true){
			    customerString = customerJSON;
			    } else {
					    customerString = "";
					};
			  var departmentInput = document.getElementById("departmentSelect").value;
			  	if (departmentSelect.selected == true){
			    departmentString = departmentJSON;
			    } else {
					    departmentString = "";
					};
			  var environmentInput = document.getElementById("environmentSelect").value;
			  	if (environmentSelect.selected == true){
			    environmentString = environmentJSON;
			    } else {
					    environmentString = "";
					};
			  var functionInput = document.getElementById("functionSelect").value;
			  	if (functionSelect.selected == true){
			    functionString = functionJSON;
			    } else {
					    functionString = "";
					};
			  var labelInput = document.getElementById("labelSelect").value;
			  	if (labelSelect.selected == true){
			    labelString = labelJSON;
			    } else {
					    labelString = "";
					};
			  var ownerInput = document.getElementById("ownerSelect").value;
			  	if (ownerSelect.selected == true){
			    ownerString = ownerJSON;
			    } else {
					    ownerString = "";
					};
			  var productInput = document.getElementById("productSelect").value;
			  	if (productSelect.selected == true){
			    productString = productJSON;
			    } else {
					    productString = "";
					};
			  var projectInput = document.getElementById("projectSelect").value;
			  	if (projectSelect.selected == true){
			    projectString = projectJSON;
			    } else {
					    projectString = "";
					};
			  var regionInput = document.getElementById("regionSelect").value;
			  	if (regionSelect.selected == true){
			    regionString = regionJSON;
			    } else {
					    regionString = "";
					};
			  var stackInput = document.getElementById("stackSelect").value;
			  	if (stackSelect.selected == true){
			    stackString = stackJSON;
			    } else {
					    stackString = "";
					};
			  var teamInput = document.getElementById("teamSelect").value;
			  	if (teamSelect.selected == true){
			    teamString = teamJSON;
			    } else {
					    teamString = "";
					};
			  var tierInput = document.getElementById("tierSelect").value;
			  	if (tierSelect.selected == true){
			    tierString = tierJSON;
			    } else {
					    tierString = "";
					};

			  	document.getElementById("perspectiveScriptTextBox").value = applicationString + accountsubscriptionprojectString + clientString + customerString + departmentString + environmentString + functionString + labelString + ownerString + productString + projectString + regionString + stackString + teamString + tierString;
				copyPerspectiveScript();
			};

			function copyPerspectiveScript() {
			  var copyPerspectiveScriptText = document.getElementById("perspectiveScriptTextBox");
				copyPerspectiveScriptText.select();
				document.execCommand("copy");
				document.getElementById('success').style.display = 'block';
			};			
		</script>
	</div>

